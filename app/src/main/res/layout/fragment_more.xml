<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Screen.TheLoaiActivity">
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/ic_account"
        android:layout_marginTop="80dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/ic_acc"/>
        <Button
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/ic_acc"
            android:text="Login"
            android:layout_marginTop="20dp"
            android:id="@+id/btnlogin"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />
    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_help_24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlogin"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="30dp"
        android:id="@+id/ic_help"
        />
    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/tv_help"
        android:textStyle="bold"
        android:text="Hỗ trợ"
        android:textSize="15dp"
        android:gravity="center_vertical"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@+id/ic_help"
        app:layout_constraintTop_toBottomOf="@id/btnlogin"/>
    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_baseline_settings"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_help"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="30dp"
        android:id="@+id/ic_setting"
        />
    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/tv_setting"
        android:textStyle="bold"
        android:text="Cài đặt"
        android:textSize="15dp"
        android:gravity="center_vertical"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@+id/ic_help"
        app:layout_constraintTop_toBottomOf="@id/tv_help"/>
    >
    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_info_24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ic_setting"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="30dp"
        android:id="@+id/ic_info"
        />
    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/tv_info"
        android:textStyle="bold"
        android:text="Thông tin"
        android:textSize="15dp"
        android:gravity="center_vertical"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@+id/ic_info"
        app:layout_constraintTop_toBottomOf="@id/tv_setting"/>
</androidx.constraintlayout.widget.ConstraintLayout>